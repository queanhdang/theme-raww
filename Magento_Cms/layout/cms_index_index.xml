        <referenceContainer name="main">
            <block class="Magento\Framework\View\Element\Template" name="dqa.main.content" template="Magento_Cms::custom/dqa-home-content.phtml"/>
            <block class="Magento\Framework\View\Element\Template" name="dqa.newsletter" template="Magento_Cms::custom/dqa-newsletter.phtml"/>
